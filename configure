#line $LINENO "configure"
#include "confdefs.h"

#include <stdio.h>
#include <sys/types.h>
#include <pwd.h>
#include <grp.h>

static int error;

static void
bad (char *p)
{
  if (error)
    printf (" %s", p);
  else {
    error = 1;
    printf ("[%s", p);
  }
}

int
main ()
{
  struct passwd *pw = $sfs_getpwd;
  struct group *gr = $sfs_getgrp;

  if (!$uid_isnum && !pw)
    bad ("No user $sfsuser.");
  if (!$gid_isnum && !gr)
    bad ("No group $sfsgroup.");
  else if (gr && gr->gr_mem[0])
    bad ("Group $sfsgroup cannot have users.");
  if (pw && gr && pw->pw_gid != gr->gr_gid)
    bad ("Login group of $sfsuser must be $sfsgroup.");
  if (error)
    printf ("] ");
  return error;
}

